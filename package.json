{
  "name": "raymud",
  "version": "0.0.1",
  "description": "Web-based MUD game powered by nodejs & socket.io",
  "main": "index.js",
  "bin": {
    "raymud": "./dist/raymud.js"
  },
  "scripts": {
    "start": "node ./dist/raymud.js",
    "debug": "node --debug ./dist/raymud.js",
    "clean": "rm -rf dist",
    "build": "babel ./src -d ./dist -s",
    "rebuild": "npm run clean && npm run build",
    "test": "for i in $(ls src/tests/); do babel-node \"src/tests/${i}\" | faucet ; done",
    "validate": "npm run lint; npm run test && npm outdated --depth 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/floatinghotpot/raymud.git"
  },
  "keywords": [
    "mud",
    "raymud",
    "mud.js"
  ],
  "author": "Raymond Xie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/floatinghotpot/raymud/issues"
  },
  "homepage": "https://github.com/floatinghotpot/raymud#README.md",
  "dependencies": {
    "express": "^4.13.4",
    "minimist": "^1.2.0",
    "mixin": "^0.2.0",
    "redis": "^2.4.2",
    "socket.io": "^1.4.4",
    "socket.io-redis": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "faucet": "0.0.1",
    "tape": "^4.4.0"
  }
}
